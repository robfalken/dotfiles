{
  "agent": {
    "button": false,
    "always_allow_tool_actions": true,
    "default_profile": "ask",
    "default_model": {
      "model": "claude-sonnet-4",
      "provider": "zed.dev"
    }
  },
  "git_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "search": {
    "button": false
  },
  "scroll_beyond_last_line": "off",
  "buffer_font_fallbacks": ["Symbols Nerd Font Mono"],
  "buffer_font_family": "Zed Plex Mono",
  "buffer_font_size": 12,
  "buffer_font_weight": 100,
  "cursor_blink": false,
  "extend_comment_on_newline": false,
  "unnecessary_code_fade": 0.6,
  "_experimental.theme_overrides": {
    // Backgrounds
    "background": "#171821",
    "element.active": "#0f0",
    "element.background": "#32344D",
    "element.selected": "#1F202A",
    "element.hover": "#262630",
    "toolbar.background": "#171821",
    "editor.background": "#1E1F2A",
    "editor.gutter.background": "#1C1D27",
    "panel.background": "#15161E",
    "terminal.background": "#1E1F2A",
    "status_bar.background": "#13141B",
    "title_bar.background": "#13141B",
    "elevated_surface.background": "#1F202A",
    "tab_bar.background": "#191A23",
    "tab.inactive_background": "#191A23",
    "tab.active_background": "#1E1F2A",
    "editor.active_line.background": "#1A1A24",
    "predictive.border": "#f00",
    "predictive.background": "#f00",
    "predictive": "#473D5A",
    "info.border": "#33488A",
    "error.border": "#0f0",
    "link_text.hover": "#F5FF69",
    "editor.document_highlight.bracket_background": "#322E46",
    "ghost_element.hover": "#262630",
    "ghost_element.active": "#3B4091",
    "ghost_element.background": "#00000000",
    "ghost_element.selected": "#15161E",
    // Icons
    "icon": "#A2A6E1",
    "icon.accent": "#f00",
    "icon.muted": "#f00",
    "icon.disabled": "#f00",
    "icon.placeholder": "#f00",
    // Text
    "text.accent": "#6369CE",
    "text.muted": "#444455",
    "editor.line_number": "#3E405B",
    "editor.active_line_number": "#8E89A6",
    "error": "#FF6769",
    "hint": "#47496B",
    "hint.background": "#3C5595",
    "warning": "#FFC44F",
    "warning.background": "#E88000",
    // Borders & lines
    "border": "#262731",
    "border.variant": "#232430",
    "editor.indent_guide": "#1D1E25",
    "editor.indent_guide_active": "#262731",
    // Terminal
    "terminal.ansi.bright_black": "#616177",
    "players": [
      {
        "cursor": "#6369CE",
        "selection": "#6369CE4D"
      }
    ],
    "syntax": {
      "comment": {
        "font_style": "italic",
        "color": "#454763"
      },
      "title": {
        "font_style": "italic",
        "color": "#A2A6E1"
      },
      "string": {
        "color": "#FFC44F"
      },
      "string.special": {
        "color": "#6ACCB4"
      },
      "function.method": {
        "color": "#B5B8E8"
      },
      "function.special.definition": {
        "color": "#f00"
      },
      "punctuation.delimiter": {
        "color": "#8C82B8"
      },
      "punctuation.bracket": {
        "color": "#8C82B8"
      },
      "punctuation.list_marker": {
        "color": "#FF77AE"
      },
      "punctuation.special": {
        "color": "#FF814F"
      },
      "punctuation": {
        "color": "#f00"
      },
      "keyword": {
        "color": "#FF77AE"
      },
      "property": {
        "color": "#B5B8E8"
      },
      "property.json_key": {
        "color": "#7A7FD5"
      },
      "tag": {
        "color": "#7A7FD5"
      },
      "attribute": {
        "color": "#A2A6E1"
      },
      "boolean": {
        "color": "#FF77AE"
      },
      "constant": {
        "color": "#FF945F",
        "font_style": "italic"
      },
      "number": {
        "color": "#F5FF69"
      },
      "type.builtin": {
        "color": "#FFE969"
      },
      "type": {
        "color": "#7A7FD5"
      }
    },
    // WTFs
    "unreachable": "#f00",
    "surface.background": "#1E1F2A",
    "border.focused": "#0f0",
    "border.selected": "#f00",
    "pane.focused_border": "#0f0",
    "panel.focused_border": "#6369CE",
    "border.disabled": "#f00",
    "editor.hover_line_number": "#f00"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "file_types": {
    "Shell Script": ["env", "envrc"],
    "TOML": ["**/ghostty/config"],
    "tmux": ["**/.tmux.conf"]
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "icon_theme": "Catppuccin Frappé",
  "journal": {
    "hour_format": "hour24",
    "path": "~/journal"
  },
  "languages": {
    "Ruby": {
      "formatter": {
        "external": {
          "arguments": ["-x"],
          "command": "rufo"
        }
      }
    },
    "Rust": {
      "inlay_hints": {
        "enabled": true
      },
      "show_edit_predictions": true
    },
    "TSX": {
      "show_edit_predictions": true
    },
    "TypeScript": {
      "show_edit_predictions": true
    }
  },
  "lsp": {
    "deno": {
      "settings": {
        "deno": {
          "enable": false
        }
      }
    }
    // "rust-analyzer": {
    // "initialization_options": {
    //   "inlayHints": {
    //     "closureReturnTypeHints": {
    //       "enable": "always"
    //     },
    //     "lifetimeElisionHints": {
    //       "enable": "skip_trivial",
    //       "useParameterNames": true
    //     },
    //     "maxLength": null
    //   }
    // }
    // }
  },
  "notification_panel": {
    "button": false
  },
  "project_panel": {
    "button": false,
    "dock": "right"
  },
  "relative_line_numbers": true,
  "scrollbar": {
    "show": "never"
  },
  "show_edit_predictions": false,
  "show_whitespaces": "boundary",
  "tab_bar": {
    "show": false
  },
  "tab_size": 2,
  "terminal": {
    "button": false,
    "font_family": "Zed Plex Mono",
    "line_height": {
      "custom": 1
    }
  },
  "theme": "Rosé Pine Moon",
  "ui_font_size": 14,
  "vim": {
    "use_system_clipboard": "never"
  },
  "vim_mode": true
}
