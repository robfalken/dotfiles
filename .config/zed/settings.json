{
  "assistant": {
    "default_model": {
      "model": "claude-3-7-sonnet-latest",
      "provider": "zed.dev"
    },
    "version": "2"
  },
  "buffer_font_fallbacks": [
    "Symbols Nerd Font Mono"
  ],
  "buffer_font_family": "Zed Plex Mono",
  "buffer_font_size": 12,
  "buffer_font_weight": 100,
  "cursor_blink": false,
  "experimental.theme_overrides": {
    "background": "#26242E",
    "editor.active_line.background": "#1D1B25",
    "editor.active_line_number": "#8E89A6",
    "editor.background": "#26242E",
    "editor.foreground": "#E0DDE4",
    "editor.gutter.background": "#201E28",
    "editor.indent_guide": "#534B74",
    "editor.indent_guide_active": "#E0DDE4",
    "editor.line_number": "#534B74",
    "invisible": "#534B74",
    "players": [
      {
        "cursor": "#FFC260",
        "selection": "#FFC26032"
      }
    ],
    "syntax": {
      "attribute": {
        "color": "#FF577F"
      },
      "comment": {
        "color": "#665783"
      },
      "hint": {
        "color": "#473D5A"
      },
      "keyword": {
        "color": "#B4A2DB"
      },
      "property": {
        "color": "#96ECBC"
      },
      "string": {
        "color": "#27BF69"
      },
      "type": {
        "color": "#FFC260"
      }
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "file_types": {
    "Shell Script": [
      "env",
      "envrc"
    ]
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    }
  },
  "icon_theme": "Catppuccin Frapp√©",
  "journal": {
    "hour_format": "hour24",
    "path": "~/journal"
  },
  "languages": {
    "Ruby": {
      "formatter": {
        "external": {
          "arguments": [
            "-x"
          ],
          "command": "rufo"
        }
      }
    },
    "Rust": {
      "inlay_hints": {
        "enabled": true
      },
      "show_edit_predictions": true
    },
    "TSX": {
      "show_edit_predictions": true
    },
    "TypeScript": {
      "show_edit_predictions": true
    }
  },
  "lsp": {
    "deno": {
      "settings": {
        "deno": {
          "enable": false
        }
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "inlayHints": {
          "closureReturnTypeHints": {
            "enable": "always"
          },
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "maxLength": null
        }
      }
    }
  },
  "notification_panel": {
    "button": false
  },
  "project_panel": {
    "button": false,
    "dock": "right"
  },
  "relative_line_numbers": true,
  "scrollbar": {
    "show": "never"
  },
  "show_edit_predictions": false,
  "show_whitespaces": "boundary",
  "tab_bar": {
    "show": false
  },
  "tab_size": 2,
  "terminal": {
    "button": false,
    "font_family": "Zed Plex Mono",
    "line_height": "standard"
  },
  "theme": "NSFW",
  "ui_font_size": 14,
  "vim": {
    "use_system_clipboard": "never"
  },
  "vim_mode": true
}